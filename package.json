{
  "name": "pali_coding_challenge",
  "version": "1.0.0",
  "description": "A NodeJS web service that accepts a list of meal IDs and returns the ID of the meal that requires the least number of ingredients to prepare. Makes use of TheMealDb.com API.",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "devstart": "nodemon --exec babel-node ./src/index.js",
    "test": "nyc npx mocha ./test/**/*.js --require @babel/register",
    "build-babel": "babel --out-dir ./build --source-maps --copy-files ./src",
    "build": "mkdir build && npm run build-babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mecsoccer/Pali_Coding_challenge.git"
  },
  "keywords": [
    "es6",
    "nodejs"
  ],
  "author": "Jaachimmma Onyenze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mecsoccer/Pali_Coding_challenge/issues"
  },
  "homepage": "https://github.com/mecsoccer/Pali_Coding_challenge#readme",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "custom-env": "^1.0.0",
    "express": "^4.16.4",
    "mocha": "^6.0.2",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
